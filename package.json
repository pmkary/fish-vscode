{
    "name": "kary-fish",
    "displayName": "Fish shell",
    "description": "Fish shell language support",
    "icon": "fish_icon.svg",
    "version": "0.0.4",
    "homepage": "https://github.com/pmkary/fish-vscode",
    "repository": {
        "type": "git",
        "url": "https://github.com/pmkary/fish-vscode.git"
    },
    "bugs": {
        "email": "pouya@kary.us",
        "url": "https://github.com/pmkary/fish-vscode/issues"
    },
    "publisher": "karyfoundation",
    "galleryBanner": {
        "color": "#312C3B",
        "theme": "dark"
    },
    "license": "MIT",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "fish",
            "aliases": ["fish", "fish"],
            "extensions": [".fish"],
            "firstLine": "^#!.*\\bfish",
            "configuration": "./fish.configuration.json"
        }],
        "grammars": [{
            "language": "fish",
            "scopeName": "source.fish",
            "path": "./syntaxes/fish.tmLanguage"
        }],
        "snippets": [
            {
                "language": "fish",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}
